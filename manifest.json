{
  "manifest_version": 2, 
  "name": "pallonizer", 
  "description": "A fun chrome extension to turn images and videos on the page to Pallone memez!", 
  "version": "10.02", 
  "content_scripts" : [
    {
      "matches": ["<all_urls>"], 
      "run_at": "document_idle", 
      "js": ["src/memeing.js"]
    }
  ],
  "browser_action": {
    "default_icon": "image.png",
    "default_popup": "src/popup.html",
    "default_title": "My First Chrome Extension"
  }, 
  "permissions": [
    "storage"
  ]
}